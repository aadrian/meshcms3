{
  "template": "recaptcha.jsp",
  "beanClass": "com.cromoteca.meshcms.server.modules.Captcha",
  "parameters": [
    {
      "name": "theme",
      "type": "SELECTION",
      "values": [
        "red",
        "white",
        "blackglass",
        "clean"
      ],
      "defaultValue": "red",
      "siteWide": true
    },
    {
      "name": "showToKnownUsers",
      "type": "BOOLEAN",
      "defaultValue": "true",
      "siteWide": true
    },
    {
      "name": "publicKey",
      "type": "TEXT",
      "siteWide": true
    },
    {
      "name": "privateKey",
      "type": "TEXT",
      "siteWide": true
    }
  ]
}
