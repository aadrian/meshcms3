{
  "beanClass": "com.cromoteca.meshcms.server.modules.PageList",
  "parameters": [
    {
      "name": "path",
      "type": "PATH"
    },
    {
      "name": "numberOfEntries",
      "type": "INTEGER",
      "required": true,
      "defaultValue": "5"
    },
    {
      "name": "sortByVisits",
      "type": "BOOLEAN"
    },
    {
      "name": "showPageImages",
      "type": "BOOLEAN",
      "defaultValue": "true"
    },
    {
      "name": "pageImageWidth",
      "type": "INTEGER",
      "siteWide": true
    },
    {
      "name": "pageImageHeight",
      "type": "INTEGER",
      "siteWide": true
    },
    {
      "name": "showDates",
      "type": "BOOLEAN",
      "defaultValue": "true"
    },
    {
      "name": "showBodies",
      "type": "BOOLEAN"
    },
    {
      "name": "bodyExcerptLength",
      "type": "INTEGER",
      "defaultValue": "500"
    },
    {
      "name": "showReadMoreLinks",
      "type": "BOOLEAN"
    },
    {
      "name": "showTags",
      "type": "BOOLEAN"
    },
    {
      "name": "showHistoryLinks",
      "type": "BOOLEAN"
    }
  ]
}
