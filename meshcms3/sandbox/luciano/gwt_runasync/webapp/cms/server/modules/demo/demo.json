{
  "template": "demo.jsp",
  "beanClass": "com.cromoteca.meshcms.server.core.ServerModule",
  "parameters": [
    {
      "name": "sample_text",
      "type": "TEXT"
    },
    {
      "name": "sample_big_text",
      "type": "MULTILINE_TEXT"
    },
    {
      "name": "sample_rich_text",
      "type": "RICH_TEXT"
    },
    {
      "name": "sample_integer",
      "type": "INTEGER"
    },
    {
      "name": "sample_boolean",
      "type": "BOOLEAN"
    },
    {
      "name": "sample_selection",
      "type": "SELECTION",
      "values": [
        "first_choice",
        "second_choice",
        "last_choice"
      ]
    },
    {
      "name": "sample_path",
      "type": "PATH"
    }
  ]
}
