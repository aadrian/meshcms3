{
  "template": "media_player.jsp",
  "beanClass": "com.cromoteca.meshcms.server.modules.MediaPlayer",
  "parameters": [
    {
      "name": "directory",
      "type": "PATH"
    },
    {
      "name": "showVideos",
      "type": "BOOLEAN"
    },
    {
      "name": "showSongs",
      "type": "BOOLEAN"
    },
    {
      "name": "showImages",
      "type": "BOOLEAN"
    },
    {
      "name": "wide",
      "type": "BOOLEAN"
    },
    {
      "name": "order",
      "type": "SELECTION",
      "values": [
        "alphabetical",
        "newestfirst",
        "oldestfirst",
        "random"
      ],
      "defaultValue": "alphabetical"
    }
  ]
}
