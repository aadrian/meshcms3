{
  "template": "downloads.jsp",
  "beanClass": "com.cromoteca.meshcms.server.modules.Downloads",
  "parameters": [
    {
      "name": "path",
      "type": "PATH"
    },
    {
      "name": "extensions",
      "type": "TEXT"
    },
    {
      "name": "beautify",
      "type": "BOOLEAN"
    },
    {
      "name": "force",
      "type": "BOOLEAN",
      "defaultValue": true
    },
    {
      "name": "order",
      "type": "SELECTION",
      "values": [
        "alphabetical",
        "newestfirst",
        "oldestfirst",
        "random"
      ],
      "defaultValue": "alphabetical"
    }
  ]
}
